#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/amdy/eclipse-workspace/contacts/node_modules/.pnpm/@eclipse-scout+releng@10.0.3/node_modules/@eclipse-scout/releng/bin/node_modules:/home/amdy/eclipse-workspace/contacts/node_modules/.pnpm/@eclipse-scout+releng@10.0.3/node_modules/@eclipse-scout/releng/node_modules:/home/amdy/eclipse-workspace/contacts/node_modules/.pnpm/@eclipse-scout+releng@10.0.3/node_modules/@eclipse-scout/node_modules:/home/amdy/eclipse-workspace/contacts/node_modules/.pnpm/@eclipse-scout+releng@10.0.3/node_modules:/home/amdy/eclipse-workspace/contacts/node_modules/.pnpm/node_modules:/home/amdy/eclipse-workspace/contacts/node_modules:/home/amdy/eclipse-workspace/node_modules:/home/amdy/node_modules:/home/node_modules:/node_modules:/home/amdy/eclipse-workspace/contacts/node_modules/.pnpm/node_modules/@eclipse-scout/releng/bin/node_modules:/home/amdy/eclipse-workspace/contacts/node_modules/.pnpm/node_modules/@eclipse-scout/releng/node_modules:/home/amdy/eclipse-workspace/contacts/node_modules/.pnpm/node_modules/@eclipse-scout/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@eclipse-scout/releng/bin/releng-scripts.js" "$@"
else
  exec node  "$basedir/../@eclipse-scout/releng/bin/releng-scripts.js" "$@"
fi
